<section id="page-title" class="page-title-parallax page-title-dark page-title-right" style="padding: 180px 0; background-image: url(); background-color: #aaa; background-size: cover; background-position: center center;" data-stellar-background-ratio="0.2">

    <div ng-app="App">
        <div ng-controller="AppController">

            <input type="file" id="file"  class="inputfile" name="file"
                   onchange="angular.element(this).scope().photoChanged(this.files)" multiple="false"/>
            <img ng-show="thumbnail.dataUrl != null" ng-src="{{ thumbnail.dataUrl }}" class="account-thumb">
            <label class="label-file" for="file">Profile Image</label>

            <div class="container clearfix">
                <h1></h1>
            </div>
        </div>
    </div>

</section><!-- #page-title end -->

